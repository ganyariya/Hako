{
    "name": "ganyariya/hako",
    "type": "library",
    "description": "A super simple DI Container",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Ganyariya\\Hako\\": "src/"
        },
        "files": [
            "src/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/",
            "HoGame\\": "hogame/"
        }
    },
    "authors": [
        {
            "name": "ganyariya",
            "email": "ganariya2525@gmail.com"
        }
    ],
    "scripts": {
        "test": [
            "phpunit"
        ],
        "phpcs": [
            "phpcs"
        ],
        "phpstan": [
            "phpstan analyse -c phpstan.neon"
        ],
        "psalm": [
            "psalm"
        ]
    },
    "require": {
        "psr/container": "^2.0"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.7",
        "vimeo/psalm": "^4.26",
        "phpstan/phpstan": "^1.8",
        "phpunit/phpunit": "^9.5"
    }
}